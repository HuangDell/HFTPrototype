{  
    "version": "0.2.0",  
    "configurations": [  

        {  
            "name": "DPDK Debug",  
            "type": "cppdbg",  
            "request": "launch",  
            "program": "${workspaceFolder}/build/Prototype-debug",  
            "args": [],  
            "stopAtEntry": false,  
            "cwd": "${workspaceFolder}",  
            "environment": [],  
            "externalConsole": false,  
            "MIMode": "gdb",  
            "setupCommands": [  
                {  
                    "description": "为 gdb 启用整齐打印",  
                    "text": "-enable-pretty-printing",  
                    "ignoreFailures": true  
                }  
            ],  
            "preLaunchTask": "build-debug",  
            "miDebuggerPath": "${workspaceFolder}/.vscode/sudo_gdb",
        }  
    ]  
}  